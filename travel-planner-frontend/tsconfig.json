{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "jsx": "react",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./src",
      "types": ["node", "react", "react-dom"],
      "paths": {
        "*": ["*"]
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  